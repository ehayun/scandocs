<h2><%= @title %></h2>

<%= f = form_for @changeset, "#",
  id: "docgroup-form",
  phx_target: @myself,
  phx_change: "validate",
  phx_submit: "save" %>

  <%= label f, :grp_id %>
  <%= number_input f, :grp_id %>
  <%= error_tag f, :grp_id %>

  <%= label f, :grp_name %>
  <%= text_input f, :grp_name %>
  <%= error_tag f, :grp_name %>

  <%= submit "Save", phx_disable_with: "Saving..." %>
</form>
